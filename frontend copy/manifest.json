{
  "name": "FrontEnd",
  "description": "Trying out building an extension to call the scraper",
  "version": "0.2",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "sidePanel",
    "downloads",
    "tabCapture",
    "offscreen"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title":"Test",
    "default_popup":"index.html"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
